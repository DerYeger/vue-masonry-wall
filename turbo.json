{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "@yeger/vue-masonry-wall#typecheck": {
      "inputs": [
        "vite.config.ts",
        "tsconfig.json",
        "src/**/*.ts",
        "src/**/*.vue"
      ],
      "outputs": []
    },
    "demo#typecheck": {
      "dependsOn": ["@yeger/vue-masonry-wall#build"],
      "inputs": ["tsconfig.json", "demo/**/*.ts", "demo/**/*.vue"],
      "outputs": []
    },
    "@yeger/vue-masonry-wall#build": {
      "dependsOn": ["@yeger/vue-masonry-wall#typecheck"],
      "inputs": [
        "vite.config.ts",
        "tsconfig.json",
        "src/**/*.ts",
        "src/**/*.vue"
      ],
      "outputs": ["dist/**"]
    },
    "demo#build": {
      "dependsOn": ["demo#typecheck"],
      "inputs": ["demo/vite.config.ts", "tsconfig.json", "demo/**/*"],
      "outputs": ["demo/dist/**"]
    },
    "demo#preview": {
      "dependsOn": ["demo#build"],
      "cache": false
    },
    "@yeger/vue-masonry-wall#test": {
      "inputs": [
        "vite.config.ts",
        "src/**/*.ts",
        "src/**/*.vue",
        "test/**/*.ts"
      ],
      "outputs": []
    },
    "@yeger/vue-masonry-wall#test:coverage": {
      "inputs": [
        "vite.config.ts",
        "src/**/*.ts",
        "src/**/*.vue",
        "test/**/*.ts"
      ],
      "outputs": ["coverage/**"]
    },
    "dev": {
      "outputs": [],
      "cache": false
    },
    "lint": {
      "outputs": []
    }
  }
}

{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "@yeger/vue-masonry-wall#typecheck": {
      "inputs": ["src/**/*.{ts,vue}"],
      "outputs": []
    },
    "demo#typecheck": {
      "dependsOn": ["@yeger/vue-masonry-wall#build"],
      "inputs": ["demo/**/*.{ts,vue}"],
      "outputs": []
    },
    "@yeger/vue-masonry-wall#build": {
      "dependsOn": ["@yeger/vue-masonry-wall#typecheck"],
      "inputs": ["src/**/*.{ts,vue}"],
      "outputs": ["dist/**"]
    },
    "demo#build": {
      "dependsOn": ["demo#typecheck"],
      "inputs": ["demo/**/*"],
      "outputs": ["demo/dist/**"]
    },
    "demo#preview": {
      "dependsOn": ["demo#build"],
      "cache": false
    },
    "@yeger/vue-masonry-wall#test": {
      "inputs": ["src/**/*.{ts,vue}", "test/**/*.ts"],
      "outputs": []
    },
    "@yeger/vue-masonry-wall#test:coverage": {
      "inputs": ["src/**/*.{ts,vue}", "test/**/*.ts"],
      "outputs": ["./coverage/**"]
    },
    "dev": {
      "outputs": [],
      "cache": false
    },
    "lint": {
      "outputs": []
    }
  }
}
